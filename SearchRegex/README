Compile project:
    javac -d bin/ src/*.java

Run project:
    java --class-path bin Application
    java --class-path bin Application "Ioana are mere si Andrei are mai multe mere unde vezi ca Matei are Mere dar mai mere niste Mere " "mere"

Package project:
    jar cfm dist/Application.jar res/Manifest.txt -C bin .

Run package:
    java -jar dist/Application.jar
    java -jar dist/Application.jar "Ioana are mere si Andrei are mai multe mere unde vezi ca Matei are Mere dar mai mere niste Mere " "mere"
    