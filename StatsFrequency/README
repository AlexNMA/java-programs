Compile project:
    javac -d bin/ src/*.java

Run project:
    java --class-path bin Application
    java --class-path bin Application "Marian are mere si Andrei are pere dar Antonia cumpara de la amandoi"

Package project:
    jar cfm dist/Application.jar res/Manifest.txt -C bin .

Run package:
    java -jar dist/Application.jar
    java -jar dist/Application.jar "Marian are mere si Andrei are pere dar Antonia cumpara de la amandoi" 
    