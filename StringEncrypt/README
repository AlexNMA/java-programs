Compile project:
    javac -d bin/ src/*.java

Run project:
    java --class-path bin Application
    java --class-path bin Application "Test123" "3"

Package project:
    jar cfm dist/Application.jar res/Manifest.txt -C bin .

Run package:
    java -jar dist/Application.jar
    java -jar dist/Application.jar "Test123" "3"
    